# Unified .env.example for the entire project
# Copy this file to .env and fill in values as needed

# API keys
OPENAI_API_KEY=
TOPK_API_KEY=

# Database endpoints
QDRANT_URL=http://localhost:6333
MILVUS_HOST=localhost
MILVUS_PORT=19530
WEAVIATE_URL=http://localhost:8080

# Sentence Transformers model
# This model is used for generating embeddings in the UI
UI_MODEL=sentence-transformers/all-MiniLM-L6-v2

# Embeddings parquet (optional, for auto-ingest in UI backend)
EMBEDDINGS_PARQUET=./data/embeddings.parquet
